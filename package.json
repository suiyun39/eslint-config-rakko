{
  "name": "eslint-config-rakko",
  "version": "1.2.1",
  "description": "自用 ESLint 规则集",
  "repository": "github:suiyun39/eslint-config-rakko",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "start": "node ./scripts/view-config.js",
    "build": "tsc && tsup",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.0.2",
    "eslint": "^9.8.0",
    "eslint-flat-config-viewer": "^0.1.20",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "sucrase": "^3.35.0",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "eslint": "^9.8.0"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^2.6.0",
    "eslint-flat-config-utils": "^0.3.0",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-n": "17.10.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "5.1.0-rc-a7d1240c-20240731",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-solid": "^0.14.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.8.0",
    "typescript-eslint": "^8.0.0"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
