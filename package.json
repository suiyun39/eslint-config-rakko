{
  "name": "eslint-config-rakko",
  "version": "1.2.1",
  "description": "自用 ESLint 规则集",
  "repository": "github:suiyun39/eslint-config-rakko",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "type": "module",
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "start": "node ./scripts/view-config.js",
    "build": "tsc && tsup",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/eslint": "^8.56.7",
    "@types/node": "^20.12.4",
    "eslint": "^8.57.0",
    "eslint-flat-config-viewer": "^0.1.20",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "sucrase": "^3.35.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4"
  },
  "peerDependencies": {
    "eslint": "^8.57.0"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^1.7.0",
    "eslint-flat-config-utils": "^0.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsx-expressions": "^1.3.2",
    "eslint-plugin-n": "17.0.0-6",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-solid": "^0.13.2",
    "eslint-plugin-unicorn": "^52.0.0",
    "globals": "^15.0.0",
    "typescript-eslint": "^7.5.0"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
