{
  "name": "eslint-config-rakko",
  "version": "1.3.3",
  "description": "自用 ESLint 规则集",
  "repository": "github:suiyun39/eslint-config-rakko",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "type": "module",
  "packageManager": "pnpm@10.10.0",
  "scripts": {
    "start": "eslint-config-inspector --config ./eslint.inspect.js --open false",
    "build": "tsc && tsup",
    "build:inspect": "eslint-config-inspector build --config ./eslint.inspect.js --outDir ./dist-inspect",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@eslint/config-inspector": "^1.0.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.15",
    "eslint": "^9.26.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "sucrase": "^3.35.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "eslint": "^9.26.0"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^4.2.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-plugin-import-x": "^4.11.0",
    "eslint-plugin-n": "17.17.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.1.0",
    "typescript-eslint": "^8.32.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "typescript": "5"
      }
    }
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
