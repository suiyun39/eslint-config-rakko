{
  "name": "eslint-config-rakko",
  "version": "1.3.2",
  "description": "自用 ESLint 规则集",
  "repository": "github:suiyun39/eslint-config-rakko",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "type": "module",
  "packageManager": "pnpm@9.15.1",
  "scripts": {
    "start": "eslint-config-inspector --config ./eslint.inspect.js --open false",
    "build": "tsc && tsup",
    "build:inspect": "eslint-config-inspector build --config ./eslint.inspect.js --outDir ./dist-inspect",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@eslint/config-inspector": "^0.6.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "sucrase": "^3.35.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "eslint": "^9.17.0"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^2.12.1",
    "eslint-flat-config-utils": "^0.4.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.14.0",
    "typescript-eslint": "^8.18.1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
