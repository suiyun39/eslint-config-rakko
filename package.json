{
  "name": "eslint-config-rakko",
  "version": "1.3.0",
  "description": "自用 ESLint 规则集",
  "repository": "github:suiyun39/eslint-config-rakko",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "type": "module",
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "start": "eslint-config-inspector --config ./eslint.inspect.js --open false",
    "build": "tsc && tsup",
    "build:inspect": "eslint-config-inspector build --config ./eslint.inspect.js --outDir ./dist-inspect",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.4",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.5.1",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "sucrase": "^3.35.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "eslint": "^9.9.1"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^2.6.4",
    "eslint-flat-config-utils": "^0.3.1",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-n": "17.10.2",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "5.1.0-rc-7771d3a7-20240827",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-solid": "^0.14.2",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.9.0",
    "typescript-eslint": "^8.3.0"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}
