{
  "name": "eslint-config-rakko",
  "version": "1.0.1",
  "description": "自用 ESLint 规则集",
  "repository": "github:suiyun39/eslint-config-rakko",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "type": "module",
  "packageManager": "pnpm@8.15.3",
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.20",
    "eslint": "^8.56.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "sucrase": "^3.35.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "eslint": "^8.56.0"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^1.6.2",
    "globals": "^14.0.0",
    "typescript-eslint": "^7.0.2"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
